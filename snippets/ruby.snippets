snippet de Describe subject
	describe "${1:subject}" do
		${2}
	end
snippet cont Context
	context "${1:subject}" do
		${2}
	end
snippet it it block
	it "${1:does something}" do
		${2}
	end
snippet sp specify block
	specify "${1:something happens}" do
		${2}
	end
snippet be Before each test
	before do
		${1}
	end
snippet af After each test
	after do
		${1}
	end
snippet let
	let(:${1}) { ${2} }
snippet its
	its(:${1}) { ${2} }
snippet pv
	puts "${1:variable}: #{$1}"
